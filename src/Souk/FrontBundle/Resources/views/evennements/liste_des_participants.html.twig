{% extends 'FrontBundle::layout.html.twig' %}

{% block body %}
    <table class="table table-condensed">
        <thead>
        <tr>
            <th>Date reservation</th>
            <th>Client</th>
        </tr>
        </thead>
        <tbody>
        {% for p in parts %}

            <tr>
                <td>{{ p.dateRes|date('d-m-Y') }}</td>
                <td>{{ p.client.username }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}